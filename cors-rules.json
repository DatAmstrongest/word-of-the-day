[
    {
      "id": 1,
      "action": {
        "type": "modifyHeaders",
        "requestHeaders": [
          {
            "header": "Access-Control-Allow-Origin",
            "operation": "set",
            "value": "*"
          },
          {
            "header": "Access-Control-Allow-Methods",
            "operation": "set",
            "value": "GET, POST, PUT, DELETE, OPTIONS"
          }
        ]
      },
      "condition": {
        "urlFilter": "<all_urls>",
        "resourceTypes": ["main_frame", "sub_frame"]
      }
    }
  ]
  